{
    "name":"WTW/Experiments",
    "description":"WTW experiments",
    "autoload":{
        "psr-0":{
            "":"src/"
        }
    },
    "repositories":[
        {
            "type": "vcs",
            "url": "git@github.com:thierrymarianne/twitteroauth.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:thierrymarianne/tmhOAuth.git",
            "reference": "dev-master"
        }
    ],
    "require":{
        "composer/composer": "dev-master",
        "doctrine/doctrine-bundle":"dev-master",
        "doctrine/orm":"dev-master",
        "facebook/php-sdk":"3.2.1",
        "fabpot/goutte":"dev-master",
        "kertz/twitteroauth":"dev-master",
        "jms/di-extra-bundle":"dev-master",
        "jms/security-extra-bundle":"dev-master",
        "php":">=5.4.8",
        "propel/propel-bundle":"1.1.*",
        "symfony/assetic-bundle":"dev-master",
        "knplabs/knp-markdown-bundle":"1.2.*@dev",
        "symfony/monolog-bundle":"2.1.*",
        "symfony/swiftmailer-bundle":"2.1.*",
        "symfony/symfony":"2.1.*",
        "sensio/distribution-bundle":"2.1.*",
        "sensio/generator-bundle":"2.1.*",
        "sensio/framework-extra-bundle":"2.1.*",
        "themattharris/tmhOAuth":"dev-master",
        "twig/extensions":"1.0.*"
    },
    "scripts":{
        "post-install-cmd":[
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd":[
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config":{
        "bin-dir":"bin"
    },
    "minimum-stability":"dev",
    "extra":{
        "symfony-app-dir":"app",
        "symfony-web-dir":"web",
        "symfony-assets-install":"symlink",
        "symfony-vendor-dir":"vendor"
    }
}

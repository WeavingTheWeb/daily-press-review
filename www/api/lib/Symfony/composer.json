{
    "name":"WTW/Experiments",
    "description":"WTW experiments",
    "autoload":{
        "psr-0":{
            "":"src/"
        }
    },
    "repositories":[
        {
            "type":"vcs",
            "url":"git@github.com:thierrymarianne/twitteroauth.git"
        },
        {
            "type":"vcs",
            "url":"git@github.com:thierrymarianne/tmhoauth.git"
        },
        {
            "type":"vcs",
            "url":"git@github.com:thierrymarianne/guzzle.git"
        }
    ],
    "require":{
        "doctrine/doctrine-bundle":"1.2.*",
        "doctrine/doctrine-fixtures-bundle": "dev-master",
        "doctrine/orm":">=2.2.3,<2.4-dev",
        "facebook/php-sdk":"3.2.1",
        "fabpot/goutte":"dev-master",
        "friendsofsymfony/rest-bundle":"dev-master",
        "friendsofsymfony/jsrouting-bundle": "1.1.*",
        "guzzle/guzzle":"dev-master",
        "kertz/twitteroauth":"dev-master",
        "jms/di-extra-bundle":"1.3.*",
        "jms/security-extra-bundle":"1.4.*",
        "jms/serializer-bundle": "0.*",
        "sp/bower-bundle": "dev-master",
        "php":">=5.4.8",
        "propel/propel-bundle":"1.1.*",
        "symfony/assetic-bundle":"2.1.*",
        "knplabs/knp-markdown-bundle":"1.2.*@dev",
        "symfony/swiftmailer-bundle":"2.2.*",
        "symfony/monolog-bundle":"2.2.*",
        "sensio/distribution-bundle":"2.2.*",
        "sensio/generator-bundle":"2.2.*",
        "sensio/framework-extra-bundle":"2.2.*",
        "themattharris/tmhOAuth":"dev-master",
        "twig/extensions":"1.0.*"
    },
    "scripts":{
        "post-install-cmd":[
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sp\\BowerBundle\\Composer\\ScriptHandler::bowerInstall"
        ],
        "post-update-cmd":[
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sp\\BowerBundle\\Composer\\ScriptHandler::bowerInstall"
        ]

    },
    "config":{
        "bin-dir":"bin"
    },
    "minimum-stability":"dev",
    "extra":{
        "symfony-app-dir":"app",
        "symfony-web-dir":"web",
        "symfony-assets-install":"symlink",
        "symfony-vendor-dir":"vendor"
    }
}

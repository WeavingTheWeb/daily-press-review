{
    "name":"WTW/Experiments",
    "description":"WTW experiments",
    "autoload":{
        "psr-0":{
            "":"src/"
        }
    },
    "repositories":[
        {
            "type":"vcs",
            "url":"git@github.com:thierrymarianne/FOSTwitterBundle.git"
        }, {
            "type":"vcs",
            "url":"git@github.com:thierrymarianne/twitteroauth.git"
        }
    ],
    "require":{
        "behat/behat": "2.4.*@stable",
        "behat/mink": "1.4.*",
        "behat/mink-browserkit-driver":  "*",
        "behat/mink-extension": "*",
        "behat/mink-goutte-driver": "*",
        "behat/mink-selenium2-driver": "*",
        "behat/mink-zombie-driver": "1.0.*",
        "behat/symfony2-extension": "*",
        "doctrine/doctrine-bundle": "1.2.*",
        "doctrine/doctrine-fixtures-bundle": "dev-master",
        "doctrine/doctrine-migrations-bundle": "dev-master",
        "doctrine/orm": ">=2.2.3,<2.4-dev",
        "friendsofsymfony/jsrouting-bundle": "1.1.*",
        "friendsofsymfony/rest-bundle":"0.12.*",
        "friendsofsymfony/twitter-bundle": "dev-master",
        "friendsofsymfony/user-bundle": "dev-master",
        "guzzle/guzzle":"3.3.1",
        "incenteev/composer-parameter-handler": "~2.0",
        "jms/di-extra-bundle":"1.4.*",
        "jms/security-extra-bundle":"1.5.*",
        "jms/serializer-bundle": "dev-master",
        "kertz/twitteroauth":"dev-master",
        "php":">=5.4.8",
        "symfony/assetic-bundle":"2.3.*",
        "symfony/monolog-bundle":"2.3.*",
        "symfony/swiftmailer-bundle":"2.2.1",
        "symfony/symfony": "2.3.*",
        "sensio/distribution-bundle":"2.3.*",
        "sensio/framework-extra-bundle":"2.3.*",
        "sensio/generator-bundle":"2.3.*",
        "sp/bower-bundle": "*",
        "twig/extensions":"1.0.*"
    },
    "require-dev": {
        "elao/web-profiler-extra-bundle": "dev-master"
    },
    "scripts":{
        "post-install-cmd":[
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sp\\BowerBundle\\Composer\\ScriptHandler::bowerInstall"
        ],
        "post-update-cmd":[
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sp\\BowerBundle\\Composer\\ScriptHandler::bowerInstall"
        ]

    },
    "config":{
        "bin-dir":"bin"
    },
    "minimum-stability": "dev",
    "extra":{
        "symfony-app-dir":"app",
        "symfony-web-dir":"web",
        "symfony-assets-install":"symlink",
        "symfony-vendor-dir":"vendor"
    }
}

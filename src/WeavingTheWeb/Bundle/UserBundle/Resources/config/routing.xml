<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="weaving_the_web_user_show_settings" pattern="/settings" schemes="%channel%">
        <default key="_controller">WeavingTheWebUserBundle:Settings:show</default>
    </route>
    <route id="weaving_the_web_user_save_settings" pattern="/save-settings" schemes="%channel%">
        <default key="_controller">WeavingTheWebUserBundle:Settings:save</default>
    </route>
</routes>
